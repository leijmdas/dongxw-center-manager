<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet name="Sheet1">
        <section startRow="0" endRow="0"/>
        <loop startRow="1" endRow="1" items="marketRecordList" var="marketRecord" varType="com.tf.erp.biz.dto.MarketRecordDTO">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">marketRecord.areaName</mapping>
                <mapping row="1" col="1">marketRecord.marketName</mapping>
                <mapping row="1" col="2">marketRecord.productTypeName</mapping>
                <mapping row="1" col="3">marketRecord.productName</mapping>
                <mapping row="1" col="4">marketRecord.sku</mapping>
                <mapping row="1" col="5">marketRecord.maxPrice</mapping>
				<mapping row="1" col="6">marketRecord.minPrice</mapping>
				<mapping row="1" col="7">marketRecord.avgPrice</mapping>
				<mapping row="1" col="8">marketRecord.typeName</mapping>
				<mapping row="1" col="9">marketRecord.marketTime</mapping>
				<mapping row="1" col="10">marketRecord.remark</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>